{
  "expo": {
    "name": "BeastFood",
    "slug": "beastfood-mobile",
    "version": "1.0.0",
    "scheme": "beastfood",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beastfood.mobile",
      "buildNumber": "1"
    },
